{
    "index_patterns": ["app-*"],
    "settings": {
      "number_of_shards": 2
    },
    "version": 1,
    "mappings": {
        "_doc": {
            "dynamic_templates": [
                {
                    "structured.@timestamp": {
                        "match": "structured.@timestamp*",
                        "match_mapping_type": "date",
                        "mapping": {
                            "date_@timestamp": {
                                "type": "date",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "ignore_above": 256
                                    }
                                },
                                "format": "yyyy-MM-dd HH:mm:ss,SSSZ||yyyy-MM-dd'T'HH:mm:ss.SSSSSSZ||yyyy-MM-dd'T'HH:mm:ssZ||dateOptionalTime"
                            }
                        }
                    },
                    "structured.application": {
                        "match": "structured.application*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_application": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.error.message": {
                        "match": "structured.error.message*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_message": {
                                "type": "text",
                                "norms": false
                            }
                        }
                    },
                    "structured.error.stacktrace": {
                        "match": "structured.error.stacktrace*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_stacktrace": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 512
                                    }
                                }
                            }
                        }
                    },
                    "structured.error.type": {
                        "match": "structured.error.type*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_type": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.level": {
                        "match": "structured.level*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_level": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 50
                                    }
                                }
                            }
                        }
                    },
                    "structured.location.class": {
                        "match": "structured.location.class*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_class": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.location.file": {
                        "match": "structured.location.file*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_file": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.location.line_number": {
                        "match": "structured.location.line_number*",
                        "match_mapping_type": "long",
                        "mapping": {
                            "long_line_number": {
                                "type": "integer"
                            }
                        }
                    },
                    "structured.location.method": {
                        "match": "structured.location.method*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_method": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.logger_name": {
                        "match": "structured.logger_name*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_logger_name": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    },
                    "structured.message": {
                        "match": "structured.message*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "message": {
                                "type": "text"
                            }
                        }
                    },
                    "structured.thread_name": {
                        "match": "structured.thread_name*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "string_thread_name": {
                                "type": "text",
                                "fields": {
                                    "raw": {
                                        "type": "keyword",
                                        "index": false,
                                        "ignore_above": 256
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    }
}